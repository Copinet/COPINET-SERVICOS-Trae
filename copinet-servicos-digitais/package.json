  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "app": "expo start",
    "tunnel:expo": "expo start --tunnel",
    "tunnel:web": "node scripts/ngrok-web.js",
    "export:web": "expo export -p web --output-dir web-build",
