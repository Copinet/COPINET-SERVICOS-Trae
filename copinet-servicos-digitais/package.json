    "expo-location": "~16.5.4",
    "expo-notifications": "~0.29.0",
    "expo-router": "^3.5.20",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "pdfjs-dist": "^5.4.624",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.0",
    "react-native": "0.74.0",
    "react-native-safe-area-context": "4.8.0",
    "react-native-screens": "~3.30.0",
    "react-native-web": "~0.19.10",
    "tesseract.js": "^5.0.0"
  },
  "devDependencies": {
